generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int       @id @default(autoincrement())
  firstName     String   
  lastName      String
  phoneNumber   String
  email         String    @unique
  password      String
  gender        String
  interests     Interest[]
}

model Interest { 
  id          Int        @id @default(autoincrement()) 
  name        String 
  description String
  users       User[] 
}



